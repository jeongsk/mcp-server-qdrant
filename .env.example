# 옵시디언 노트 경로 (필수)
OBSIDIAN_PATH=/path/to/your/obsidian/vault

# Qdrant 설정 (QDRANT_URL 또는 QDRANT_LOCAL_PATH 중 하나는 필수)
QDRANT_URL=http://localhost:6333
# QDRANT_LOCAL_PATH=/path/to/qdrant/storage
QDRANT_API_KEY=your_api_key_if_needed

# 컬렉션 이름 (선택, 기본값: obsidian-notes)
COLLECTION_NAME=obsidian-notes

# 임베딩 모델 설정 (선택, 기본값: sentence-transformers/all-MiniLM-L6-v2)
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# 청킹 설정 (선택)
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
